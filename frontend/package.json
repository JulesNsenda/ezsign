    "axios": "^1.12.2",
    "pdfjs-dist": "^5.4.296",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.64.0",
    "react-pdf": "^10.1.0",
    "react-rnd": "^10.5.2",
    "react-router-dom": "^7.9.3",
    "signature_pad": "^5.1.1",
